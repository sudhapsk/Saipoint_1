<weblogic-web-app>
  <container-descriptor>
    <prefer-application-packages>
      <package-name>javax.faces.*</package-name>
      <package-name>com.google.*</package-name>
      <package-name>com.sun.faces.*</package-name>
      <package-name>com.sun.jersey.*</package-name>
      <package-name>org.glassfish.jersey.*</package-name>
      <package-name>javax.ws.rs.*</package-name>
      <package-name>org.apache.myfaces.*</package-name>
      <package-name>org.opensaml.*</package-name>
      <package-name>org.sl4j.*</package-name>
      <package-name>org.hibernate.*</package-name>
      <package-name>antlr.*</package-name>
      <package-name>javax.el.*</package-name>
      <package-name>com.sun.el.*</package-name>
    </prefer-application-packages>
    <prefer-application-resources>
      <resource-name>META-INF/services/com.sun.faces.spi.FacesConfigResourceProvider</resource-name>
      <resource-name>META-INF/services/org.glassfish.jersey.*</resource-name>
      <resource-name>com.sun.faces.*</resource-name>
      <resource-name>org.glassfish.jersey.*</resource-name>
      <!--<resource-name>META-INF/services/javax.servlet.ServletContainerInitializer</resource-name>-->
      <resource-name>META-INF/services/com.sun.faces.*</resource-name>
      <resource-name>META-INF/mojarra_ext.taglib.xml</resource-name>
      <resource-name>org.slf4j.*</resource-name>
      <resource-name>soap11-config.xml</resource-name>
      <resource-name>schema/*</resource-name>
    </prefer-application-resources>
  </container-descriptor>
</weblogic-web-app>
